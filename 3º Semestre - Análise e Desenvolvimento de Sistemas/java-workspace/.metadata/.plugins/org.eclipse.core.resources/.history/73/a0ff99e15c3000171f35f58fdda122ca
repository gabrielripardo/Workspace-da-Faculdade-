package prova_questoes;

public class Conta {
	double saldo;
	double numero;
	
	public void sacar(double valor){
		if (saldo>=valor){
			saldo = saldo - valor;
			System.out.println("Saque realizado com sucesso!");
		}
	}	
	public void depositar(double valor){
		saldo = saldo + valor;
		System.out.println("Dep√≥sito realizado com sucesso!");
	}			
}
